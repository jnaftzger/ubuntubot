This project requires a configuration file called 'ubuntubot.cfg' with the following format:

[powerschoolmirror]
ip:
database:
user:
password:

[gmail]
user:
password:

[powerschool]
server_root:
pw_page:
username_password:

[prowl]
email:

[vlad]
table_directory: 
download_directory: 

[ps1000]
path_to_host_file: 

[follett]
destination_directory: 